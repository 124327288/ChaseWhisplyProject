    <!-- Tuto Fragment Page Welcome -->
    <string name="tuto_page_welcome_title">Welcome,</string>
    <string name="tuto_page_welcome_subtitle">padawan.</string>
    <string name="tuto_page_welcome_content">Your mission is to seek, find and kill
        ghosts living around you.
    </string>
    <string name="tuto_page_welcome_additional_text">Swipe left to learn more.</string>

    <!-- Tuto Fragment Page Play Button -->
    <string name="tuto_page_play_button_content">Select your favorite mode and hunt ghosts around you.</string>

    <!-- Tuto Fragment Page Leaderboard Button -->
    <string name="tuto_page_leaderboard_button_content">Compare your score with your friends and try to be the best
        hunter in the World.
    </string>

    <!-- Tuto Fragment Page Achievement Button -->
    <string name="tuto_page_achievement_button_content">Improve yourself as a ghost hunter and keep track of your
        progression.
    </string>

    <!-- Tuto Fragment Page Aim Tip -->
    <string name="tuto_page_aim_tip_content">In order to aim a ghost with ease, try to picture your phone as a gun
        sights. Move your arms, not only your hands.
    </string>

    <!-- Tuto Fragment Page Target -->
    <string name="tuto_page_target_content">Your targets are Whisply and his twin brothers.</string>
    <string name="tuto_page_target_bis_content">Line up one of them in your gun sights and shoot by touching the
        screen.
    </string>

    <!-- Tuto Fragment Page Ammo -->
    <string name="tuto_page_ammo_content">Watch out your ammunitions on the bottom right of your screen, or you will run
        out of them.
    </string>
    <string name="tuto_page_ammo_bis_content">You gain ammunitions over time.</string>

